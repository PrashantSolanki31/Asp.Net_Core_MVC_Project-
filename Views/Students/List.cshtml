@model List<MVC_Week2_Project1.Models.Student>

<h1>Student List</h1>

<table class="table">
    <thead>
        <tr>
        
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Subscribed</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.Phone</td>
                <td>@student.Subscribed</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Students" asp-action="Edit" asp-route-id="@student.Id">Edit</a>
                </td>
               
            </tr>
        }
    </tbody>
</table>
<a href="/Students/Add" class="btn btn-primary">Add New Student</a>